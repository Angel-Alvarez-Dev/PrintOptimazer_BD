# Entorno de PrintOptimizer_BD

# URL de conexión a la base de datos (formato SQLAlchemy)
DATABASE_URL=postgresql://user:password@localhost:5432/printoptimizer_db

# URL de Redis (broker y backend de Celery)
REDIS_URL=redis://localhost:6379/0

# Clave de API para servicios de IA (p. ej. OpenAI)
OPENAI_API_KEY=your_openai_api_key_here

# JWT
JWT_SECRET=your_jwt_secret_here
ACCESS_TOKEN_EXPIRE_MINUTES=60

# Orígenes permitidos para CORS (separados por comas)
CORS_ORIGINS=http://localhost:3000,http://localhost:8000
